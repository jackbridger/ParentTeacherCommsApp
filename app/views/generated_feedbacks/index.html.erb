<p id="notice"><%= notice %></p>

<h1>Generated Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Performance grade</th>
      <th>Teacher</th>
      <th>Subject</th>
      <th>Auto feedback</th>
      <th>Subcategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @generated_feedbacks.each do |generated_feedback| %>
      <tr>
        <td><%= generated_feedback.performance_grade %></td>
        <td><%= generated_feedback.teacher %></td>
        <td><%= generated_feedback.subject %></td>
        <td><%= generated_feedback.auto_feedback %></td>
        <td><%= generated_feedback.subcategory %></td>
        <td><%= link_to 'Show', generated_feedback %></td>
        <td><%= link_to 'Edit', edit_generated_feedback_path(generated_feedback) %></td>
        <td><%= link_to 'Destroy', generated_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Generated Feedback', new_generated_feedback_path %>
